{
  "name": "Get JSON",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "background": {
    "scripts": [ "background.js" ]
  },
  "browser_action":{
    "default_title": "JsonReqLogger"
  },
  "permissions": [ "webRequest", "webRequestBlocking", "webNavigation", "tabs", "http://*/*", "https://*/*", "debugger"]
}