#!/usr/bin/env node
"use strict";

require('babel-polyfill');
require('babel-register')({
    presets: ['es2015','stage-3'],
    comments: false,
    only: function(filename) {
        if (/bungle\/(?!node_modules)/.test(filename)) {
            return true;
        }
        if (/esx-bower\/(?!node_modules)/.test(filename)) {
            return true;
        }
        return false;
    }
});
var Cli = require('../lib/cli').Cli;
new Cli().run();
